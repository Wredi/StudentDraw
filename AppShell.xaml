<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="StudentDraw.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:StudentDraw.Pages"
    Shell.FlyoutBehavior="Disabled">
    
    <ShellContent
        Title="StudentsBoardPage"
        ContentTemplate="{DataTemplate pages:StudentsBoardPage}"
        Route="StudentsBoardPage"/>
    
    <ShellContent
        Title="ManageStudentsPage"
        ContentTemplate="{DataTemplate pages:ManageStudentsPage}"
        Route="ManageStudentsPage"/>
    
</Shell>
